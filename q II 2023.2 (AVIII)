#include <stdio.h>
#define altura 9
#define largura 14

int main()
{
    char mtz [altura] [largura] = {{ '.', '.', '.', '.', '.', '.', '.', 'o', '.', '.', '.', '.', '.', '.' },
                                   { '.', '#', '#', '#', '.', '.', '.', '#', '#', '#', '.', '#', '.', '#' },
                                   { '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.' },
                                   { '.', '.', '#', '#', '#', '#', '#', '#', '.', '.', '.', '.', '.', '.' },
                                   { '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.' },
                                   { '.', '#', '.', '#', '#', '#', '#', '.', '.', '.', '.', '#', '#', '.' },
                                   {{ '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.' },
                                   { '.', '#', '#', '#', '.', '.', '.', '#', '#', '#', '.', '#', '.', '#' },
                                   { '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.', '.' },
    int i, j;
    
    for (i=0; i<altura; i++){
        for (j=0; j<largura; j++){
            if (mtz [i] [j] == '.' && mtz [i-1] [j] == 'o' || mtz [i] [j-1] == 'o' && mtz [i+1] [j-1] == '#' || mtz [i] [j+1] == 'o' && mtz [i+1] [j+1] == '#')
            mtz [i] [j] = 'o';
        }
    }
}
